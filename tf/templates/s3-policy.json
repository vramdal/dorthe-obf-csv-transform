{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "PublicReadGetObject",
      "Effect": "Allow",
      "Principal": "*",
      "Action": ["s3:GetObject", "s3:ListBucket"],
      "Resource": ["arn:aws:s3:::${bucket}", "arn:aws:s3:::${bucket}/*"]
    },
    {
      "Action": "s3:*",
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::310400525929:role/deploy-dorthe-obf-csv-transform"
    },
      "Resource": [
        "arn:aws:s3:::s3-website-dorthe-obf.aws.vidarramdal.com",
        "arn:aws:s3:::s3-website-dorthe-obf.aws.vidarramdal.com/*"
      ],
      "Sid": "DeployWebsite"
    }
  ]
}
